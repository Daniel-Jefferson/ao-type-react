{
 "name": "AutonomousOrganization",
 "version": "0.1.0",
 "repository": "github.com/AutonomousOrganization",
 "license": "GPL-3.0",
 "scripts": {
  "serve": "vue-cli-service serve",
  "buildFront": "vue-cli-service build",
  "compile": "npm run buildFront && npm run start",
  "setup": "chmod u+x install.sh && ./install.sh",
  "start": "node src/server/app.js",
  "pretty": "prettier --write \"src/**/*.js\"",
  "pretty-vue": "prettier --parser \"vue\" --write \"src/**/*.vue\"",
  "precommit": "lint-staged",
  "prepush": "lint-staged"
 },
 "lint-staged": {
  "*.js": [
   "npm run pretty"
  ],
  "*.vue": [
   "npm run pretty-view"
  ]
 },
 "main": "production/server/app.js",
 "dependencies": {
  "better-sqlite3": "^5.4.3",
  "body-parser": "^1.19.0",
  "cron": "^1.7.2",
  "express": "^4.17.1",
  "hammerjs": "^2.0.8",
  "kefir": "^3.8.6",
  "lightning-client": "^0.5.2-beta.8",
  "linkifyjs": "^2.1.8",
  "lodash": "^4.17.15",
  "moment": "^2.24.0",
  "path": "^0.12.7",
  "propagating-hammerjs": "^1.4.7",
  "qrcode-generator": "^1.4.4",
  "socket.io": "^2.3.0",
  "socket.io-client": "^2.3.0",
  "socketio-auth": "^0.1.1",
  "superagent": "^5.2.1",
  "tor-request": "^3.1.0",
  "vue": "^2.6.11",
  "vue-markdown": "^2.2.4",
  "vue-router": "^3.1.5",
  "vuex": "^3.1.2"
 },
 "devDependencies": {
  "@vue/cli-plugin-babel": "^4.1.2",
  "@vue/cli-service": "^4.1.2",
  "husky": "^4.2.3",
  "jest": "^25.1.0",
  "lint-staged": "^10.0.8",
  "prettier": "^1.19.1",
  "pug": "^2.0.4",
  "pug-plain-loader": "^1.0.0",
  "stylus": "^0.54.7",
  "stylus-loader": "^3.0.2",
  "vue-template-compiler": "^2.6.11",
  "webpack": "^4.41.5"
 },
 "babel": {
  "presets": [
   "@vue/app"
  ]
 },
 "postcss": {
  "plugins": {
   "autoprefixer": {}
  }
 },
 "browserslist": [
  "> 1%",
  "last 2 versions",
  "not ie <= 8"
 ]
}
