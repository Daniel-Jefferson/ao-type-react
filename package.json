{
  "name": "AutonomousOrganization",
  "version": "0.1.0",
  "repository": "github.com/AutonomousOrganization",
  "license": "GPL-3.0",
  "scripts": {
    "webdev": "webpack-dev-server --mode development --hot --progress --colors --port 3000 --open",
    "webpack": "webpack -p --progress --colors",
    "setup": "chmod u+x install.sh && ./install.sh",
    "start": "run-p webdev server",
    "server": "node src/server/app.js",
    "pretty": "npm run pretty-js && npm run pretty-vue",
    "pretty-js": "prettier --write \"src/**/*.js\"",
    "pretty-vue": "prettier --parser \"vue\" --write \"src/**/*.vue\"",
    "precommit": "lint-staged",
    "prepush": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty-js"
    ],
    "*.vue": [
      "npm run pretty-view"
    ]
  },
  "main": "production/server/app.js",
  "dependencies": {
    "@most/core": "^1.5.0",
    "@most/dom-event": "^2.2.0",
    "@most/scheduler": "^1.2.3",
    "@types/socket.io": "^2.1.4",
    "@types/superagent": "^4.1.7",
    "better-sqlite3": "^5.4.3",
    "body-parser": "^1.19.0",
    "cron": "^1.7.2",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.7.3",
    "kefir": "^3.8.6",
    "lightning-client": "^0.5.2-beta.8",
    "linkifyjs": "^2.1.8",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "propagating-hammerjs": "^1.4.7",
    "qrcode-generator": "^1.4.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "socketio-auth": "^0.1.1",
    "superagent": "^5.2.1",
    "tor-request": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "@types/node": "^13.9.2",
    "@types/webpack": "^4.4.23",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
